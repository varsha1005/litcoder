def noPrefix(words):
    for i in range (len(words)):
        for j in range(i+1,len(words)):
            if words[i]!=words[j] and words[j].startswith(words[i]):
                print("BAD PASSWORD")
                return

    print("GOOD PASSWORD")

words=input()
res=noPrefix(words)
print(res)
                                                                                                                            
